{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Database": {
    "Provider": "SqlServer",
    "ConnectionString": "Server=db;Database=EMRNextDb;User=sa;Password=${DB_PASSWORD};TrustServerCertificate=true",
    "CommandTimeout": 30,
    "EnableDetailedErrors": false,
    "EnableSensitiveDataLogging": false,
    "MaxRetryCount": 5,
    "MaxRetryDelay": 30,
    "AutoMigrate": true
  }
}
